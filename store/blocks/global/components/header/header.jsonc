{
  "header#global__header": {
    "title": "Header",
    "blocks": [
      "header-layout.desktop#desktop__header",
      "header-layout.mobile#mobile__header"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop#desktop__header",
      "header-layout.mobile#mobile__header"
    ]
  },
  "header-layout.desktop#desktop__header": {
    "children": [
      "sticky-layout#desktop__header"
    ]
  },
  "header-layout.mobile#mobile__header": {
    "children": [
      "sticky-layout#mobile__header"
    ]
  },
  "sticky-layout#desktop__header": {
    "children": [
      "flex-layout.row#header__desktop__row1",
      "flex-layout.row#header__global__row-2"
    ]
  },
  "sticky-layout#mobile__header": {
    "children": [
      "flex-layout.row#header__mobile__row1",
      "flex-layout.row#header__global__row-2"
    ]
  },
  "logo#header__desktop__row1--logo": {
    "title": "Store Logo Component",
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/images/icons/header/icon_black.svg",
      "width": "32"
    }
  },
  "search-bar#header__desktop__row1--search": {
    "title": "Search desktop header row1",
    "props": {
      "maxWidth": 280,
      "placeholder": "Search products...."
    }
  },
  "minicart.v2#header__desktop__row1--minicart": {
    "title": "Mini Cart",
    "props": {
      "MinicartIcon": "icon-cart#minicart-icon"
    },
    "children": [
      "minicart-base-content"
    ]
  },
  "icon-cart#minicart-icon": {
    "title": "Cart Icon",
    "props": {
      "size": 17
    }
  },
  "flex-layout.row#header__global__row-2": {
    "title": "Main Global Header Flex Row Categories",
    "props": {
      "horizontalAlign": "left",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "marginTop": "3",
      "preserveLayoutOnMobile": true,
      "blockClass": "row2-global"
    },
    "children": [
      "flex-layout.col#header__global__row--food",
      "flex-layout.col#header__global__row--fitness",
      "flex-layout.col#header__global__row--arctic",
      "flex-layout.col#header__global__row--poolday",
      "flex-layout.col#header__global__row--hotel",
      "flex-layout.col#header__global__row--house",
      "flex-layout.col#header__global__row--nature",
      "flex-layout.col#header__global__row--closet",
      "flex-layout.col#header__global__row--beache",
      "flex-layout.col#header__global__row--mountaineering",
      "flex-layout.col#header__global__row--delivery"
    ]
  },
  "flex-layout.col#header__global__row--food": {
    "children": [
      "image#food--icon",
      "rich-text#food-label"
    ],
    "props": {
      "blockClass": "global__category--content"
    }
  },
  "image#food--icon": {
    "props": {
      "src": "assets/images/categories/fastfood.svg",
      "width": "20px",
      "height": "20px"
    }
  },
  "rich-text#food-label": {
    "props": {
      "text": "Food",
      "blockClass": "category-description"
    }
  },
